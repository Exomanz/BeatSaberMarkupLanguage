<bg xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://monkeymanboy.github.io/BSML-Docs/BSMLSchema.xsd'>
  <tab-selector tab-tag='localization' />
  <tab tags='localization' tab-name-key='LANGUAGE_EN'>
    <settings-container>
      <color-setting/>
      <dropdown-list-setting options='contents'/>
      <increment-setting/>
      <list-setting options='contents'/>
      <slider-setting/>
      <string-setting/>
      <toggle-setting/>
      <color-setting text-key='BUTTON_CAMPAIGN'/>
      <dropdown-list-setting text-key='BUTTON_SOLO' options='contents'/>
      <increment-setting text-key='BUTTON_PARTY'/>
      <list-setting text-key='BUTTON_HOW_TO_PLAY' options='contents'/>
      <slider-setting text-key='BUTTON_CREDITS'/>
      <string-setting text-key='BUTTON_SETTINGS'/>
      <toggle-setting text-key='BUTTON_PLAYER_SETTINGS'/>
    </settings-container>
  </tab>
  <tab tags='localization' tab-name-key='LANGUAGE_SC'>
    <vertical>
      <button/>
      <clickable-text/>
      <modifier/>
      <text/>
      <button text-key='BUTTON_QUIT'/>
      <clickable-text text-key='BUTTON_BASICS'/>
      <modifier text-key='BUTTON_SCORE_SYSTEM'/>
      <text text-key='BUTTON_MENU'/>
    </vertical>
  </tab>
  <tab tags='localization' tab-name-key='HEALTH_AND_SAFETY_WARNING_TITLE'>
    <text-page text-key='HEALTH_AND_SAFETY_WARNING_CONTENT'/>
  </tab>
</bg>